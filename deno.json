{
  "name": "gpt-files",
  "version": "0.0.8",
  "exports": "./cli.ts",
  "tasks": {
    "dev": "deno run -R -E -N cli.ts",
    "compile:x86_64-linux": "deno compile -R -E -N --target x86_64-unknown-linux-gnu -o dist/gpt-files-x86_64-linux cli.ts",
    "compile:arm64-linux": "deno compile -R -E -N --target aarch64-unknown-linux-gnu -o dist/gpt-files-arm64-linux cli.ts",
    "compile:x86_64-macos": "deno compile -R -E -N --target x86_64-apple-darwin -o dist/gpt-files-x86_64-macos cli.ts",
    "compile:arm64-macos": "deno compile -R -E -N --target aarch64-apple-darwin -o dist/gpt-files-arm64-macos cli.ts",
    "compile": "rm -rf ./dist && mkdir ./dist && deno task compile:x86_64-linux && deno task compile:x86_64-macos && deno task compile:arm64-linux && deno task compile:arm64-macos"
  },
  "fmt": {
    "files": {
      "include": ["cli.ts"]
    },
    "options": {
      "useTabs": false,
      "lineWidth": 80,
      "indentWidth": 2,
      "singleQuote": true
    }
  },
  "compilerOptions": {
    "lib": ["deno.window"]
  },
  "imports": {
    "@cliffy/command": "jsr:@cliffy/command@1.0.0-rc.7",
    "@cliffy/table": "jsr:@cliffy/table@1.0.0-rc.7",
    "@cliffy/ansi": "jsr:@cliffy/ansi@1.0.0-rc.7"
  }
}
